
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Tasks {
  id   String  @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  task String
  done Boolean @default(false)
}